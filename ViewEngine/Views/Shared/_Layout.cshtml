<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @RenderSection("Head", required: false)
</head>

<body>
    <header>
    @Html.Partial("~/Views/Shared/_Header.cshtml")
    </header>

    <div id="content">
    @RenderBody()
    </div>

    <footer>
    @Html.Partial("~/Views/Shared/_Footer.cshtml")
    </footer>
</body>
</html>
